    "module": "node-coglpango-12.js",
    "main": "node-coglpango-12.js",
    "typedoc": {
        "entryPoint": "./node-coglpango-12.d.ts",
        "readmeFile": "./README.md",
        "displayName": "CoglPango-12",
        "tsconfig": "./tsconfig.doc.json"
    },
    "exports": {
        ".": {
            "import": {
                "types": "./node-coglpango-12.d.ts",
                "default": "./node-coglpango-12.js"
            },
            "require": {
                "types": "./node-coglpango-12.d.cts",
                "default": "./node-coglpango-12.cjs"
            }
        }
    },
    "scripts": {
        "test": "yarn test:esm && yarn test:cjs",
        "test:esm": "tsc --noEmit node-coglpango-12.d.ts",
        "test:cjs": "tsc --noEmit node-coglpango-12.d.cts"
    },
    "dependencies": {
        "@girs/cairo-1.0": "workspace:^",
        "@girs/cogl-12": "workspace:^",
        "@girs/freetype2-2.0": "workspace:^",
