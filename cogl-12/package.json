{
    "name": "@girs/cogl-12",
    "version": "12.0.0-3.2.7",
    "description": "GJS TypeScript type definitions for Cogl-12, generated from library version 12.0.0",
    "type": "module",
    "module": "cogl-12.js",
    "main": "cogl-12.js",
    "exports": {
        "./ambient": "./cogl-12-ambient.d.ts",
        "./import": "./cogl-12-import.d.ts",
        ".": {
            "import": {
                "types": "./cogl-12.d.ts",
                "default": "./cogl-12.js"
            },
            "require": {
                "types": "./cogl-12.d.cts",
                "default": "./cogl-12.cjs"
            }
        }
    },
    "scripts": {
        "test": "yarn test:esm && yarn test:cjs",
        "test:esm": "NODE_OPTIONS=--max_old_space_size=9216 tsc --noEmit cogl-12.d.ts",
        "test:cjs": "NODE_OPTIONS=--max_old_space_size=9216 tsc --noEmit cogl-12.d.cts"
    },
    "dependencies": {
        "@girs/cairo-1.0": "workspace:^",
        "@girs/gjs": "workspace:^",
        "@girs/gl-1.0": "workspace:^",
        "@girs/glib-2.0": "workspace:^",
        "@girs/gobject-2.0": "workspace:^",
        "@girs/graphene-1.0": "workspace:^"
    },
    "devDependencies": {
        "typescript": "*"
    },
    "keywords": [
        "Gir",
        "TypeScript",
        "types",
        "GObject-Introspection",
        "GJS",
        "Cogl-12"
