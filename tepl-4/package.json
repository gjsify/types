{
    "name": "@girs/tepl-4",
    "version": "4.0.0-4.0.0",
    "description": "GJS TypeScript type definitions for Tepl-4, generated from library version 4.0.0",
    "type": "module",
    "module": "tepl-4.js",
    "main": "tepl-4.js",
    "exports": {
        "./ambient": {
            "types": "./tepl-4-ambient.d.ts",
            "import": "./tepl-4-ambient.js",
            "default": "./tepl-4-ambient.js"
        },
        "./import": {
            "types": "./tepl-4-import.d.ts",
            "import": "./tepl-4-import.js",
            "default": "./tepl-4-import.js"
        },
        ".": {
            "types": "./tepl-4.d.ts",
            "import": "./tepl-4.js",
            "default": "./tepl-4.js"
        }
    },
    "scripts": {
        "test": "NODE_OPTIONS=--max_old_space_size=9216 tsc --noEmit tepl-4.d.ts"
    },
    "dependencies": {
        "@girs/amtk-5": "workspace:^",
        "@girs/gjs": "workspace:^",
        "@girs/gtk-3.0": "workspace:^",
        "@girs/gtksource-4": "workspace:^"
    },
    "devDependencies": {
        "typescript": "*"
    },
    "keywords": [
        "Gir",
        "TypeScript",
        "types",
        "GObject-Introspection",
        "GJS",
        "Tepl-4"
    ],
    "author": "ts-for-gir",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/gjsify/ts-for-gir.git"
    },
    "bugs": {
        "url": "https://github.com/gjsify/ts-for-gir/issues"
    },
    "homepage": "https://github.com/gjsify/types/tree/main/tepl-4#readme"
}
