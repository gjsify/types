{
    "name": "@girs/tepl-4",
    "version": "4.0.0-3.2.7",
    "description": "GJS TypeScript type definitions for Tepl-4, generated from library version 4.0.0",
    "type": "module",
    "module": "tepl-4.js",
    "main": "tepl-4.js",
    "exports": {
        "./ambient": "./tepl-4-ambient.d.ts",
        "./import": "./tepl-4-import.d.ts",
        ".": {
            "import": {
                "types": "./tepl-4.d.ts",
                "default": "./tepl-4.js"
            },
            "require": {
                "types": "./tepl-4.d.cts",
                "default": "./tepl-4.cjs"
            }
        }
    },
    "scripts": {
        "test": "yarn test:esm && yarn test:cjs",
        "test:esm": "NODE_OPTIONS=--max_old_space_size=9216 tsc --noEmit tepl-4.d.ts",
        "test:cjs": "NODE_OPTIONS=--max_old_space_size=9216 tsc --noEmit tepl-4.d.cts"
    },
    "dependencies": {
        "@girs/amtk-5": "workspace:^",
        "@girs/atk-1.0": "workspace:^",
        "@girs/cairo-1.0": "workspace:^",
        "@girs/freetype2-2.0": "workspace:^",
        "@girs/gdk-3.0": "workspace:^",
        "@girs/gdkpixbuf-2.0": "workspace:^",
        "@girs/gio-2.0": "workspace:^",
        "@girs/gjs": "workspace:^",
        "@girs/glib-2.0": "workspace:^",
        "@girs/gmodule-2.0": "workspace:^",
        "@girs/gobject-2.0": "workspace:^",
        "@girs/gtk-3.0": "workspace:^",
        "@girs/gtksource-4": "workspace:^",
        "@girs/harfbuzz-0.0": "workspace:^",
        "@girs/pango-1.0": "workspace:^",
        "@girs/xlib-2.0": "workspace:^"
    },
    "devDependencies": {
        "typescript": "*"
    },
    "keywords": [
        "Gir",
        "TypeScript",
        "types",
        "GObject-Introspection",
        "GJS",
        "Tepl-4"
    ],
    "author": "ts-for-gir",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/gjsify/ts-for-gir.git"
    },
    "bugs": {
        "url": "https://github.com/gjsify/ts-for-gir/issues"
    },
    "homepage": "https://github.com/gjsify/types/tree/main/tepl-4#readme"
}
