{
    "name": "@girs/clutter-12",
    "version": "12.0.0-3.2.2",
    "description": "GJS TypeScript type definitions for Clutter-12, generated from library version 12.0.0",
    "type": "module",
    "module": "clutter-12.js",
    "main": "clutter-12.js",
    "exports": {
        "./ambient": "./clutter-12-ambient.d.ts",
        "./import": "./clutter-12-import.d.ts",
        ".": {
            "import": {
                "types": "./clutter-12.d.ts",
                "default": "./clutter-12.js"
            },
            "require": {
                "types": "./clutter-12.d.cts",
                "default": "./clutter-12.cjs"
            }
        }
    },
    "scripts": {
        "test": "yarn test:esm && yarn test:cjs",
        "test:esm": "NODE_OPTIONS=--max_old_space_size=9216 tsc --noEmit clutter-12.d.ts",
        "test:cjs": "NODE_OPTIONS=--max_old_space_size=9216 tsc --noEmit clutter-12.d.cts"
    },
    "dependencies": {
