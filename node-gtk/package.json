
{
    "name": "@girs/node-gtk",
    "version": "3.0.0",
    "description": "Node.js TypeScript type definitions for node-gtk",
    "type": "module",
    "module": "node-gtk.js",
    "main": "node-gtk.js",
    "typedoc": {
      "entryPoint": "./node-gtk.d.ts",
      "readmeFile": "./README.md",
      "displayName": "node-gtk",
      "tsconfig": "./tsconfig.doc.json"
    },
    "exports": {
      "./ambient": "./node-ambient.d.ts",
      ".": {
        "import": {
          "types": "./node-gtk.d.ts",
          "default": "./node-gtk.js"
        },
        "require": {
          "types": "./node-gtk.d.cts",
          "default": "./node-gtk.cjs"
        }
      }
    },
    "scripts": {
      "test": "yarn test:esm && yarn test:cjs",
      "test:esm": "tsc --noEmit node-gtk.d.ts",
      "test:cjs": "tsc --noEmit node-gtk.d.cts"
    },
    "dependencies": {
      
      "@girs/node-gobject-2.0": "workspace:^",            
      "@girs/node-glib-2.0": "workspace:^"      
    },
    "peerDependencies": {
      "node-gtk": "*"
    },
    "devDependencies": {
      "typescript": "^5.0.4"
    },
    "keywords": ["Gir", "TypeScript", "types", "GObject-Introspection", "node", "node-gtk", "node-gtk"],
    "author": "ts-for-gir",
    "license": "MIT",
    "repository": {
      "type": "git",
      "url": "git+https://github.com/gjsify/types.git"
    },
    "bugs": {
      "url": "https://github.com/gjsify/ts-for-gir/issues"
    },
    "homepage": "https://github.com/gjsify/types/tree/main/node-gtk#readme"
}


