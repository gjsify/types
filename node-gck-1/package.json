{
    "name": "@girs/node-gck-1",
    "version": "1.0.0-3.2.8",
    "description": "Node.js TypeScript type definitions for Gck-1, generated from library version 1.0.0",
    "type": "module",
    "module": "node-gck-1.js",
    "main": "node-gck-1.js",
    "exports": {
        "./ambient": {
            "types": "./node-gck-1-ambient.d.ts",
            "default": "./node-gck-1-ambient.js"
        },
        "./import": {
            "types": "./node-gck-1-import.d.ts",
            "default": "./node-gck-1-import.js"
        },
        ".": {
            "import": {
                "types": "./node-gck-1.d.ts",
                "default": "./node-gck-1.js"
            },
            "require": {
                "types": "./node-gck-1.d.cts",
                "default": "./node-gck-1.cjs"
            }
        }
    },
    "scripts": {
        "test": "yarn test:esm && yarn test:cjs",
        "test:esm": "NODE_OPTIONS=--max_old_space_size=9216 tsc --noEmit node-gck-1.d.ts",
        "test:cjs": "NODE_OPTIONS=--max_old_space_size=9216 tsc --noEmit node-gck-1.d.cts"
    },
    "dependencies": {
        "@girs/node-gio-2.0": "workspace:^",
        "@girs/node-glib-2.0": "workspace:^",
        "@girs/node-gobject-2.0": "workspace:^",
        "@girs/node-gtk": "workspace:^"
    },
    "peerDependencies": {
        "node-gtk": "*"
    },
    "devDependencies": {
        "typescript": "*"
    },
    "keywords": [
        "Gir",
        "TypeScript",
        "types",
        "GObject-Introspection",
        "node",
        "node-gtk",
        "Gck-1"
    ],
    "author": "ts-for-gir",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/gjsify/ts-for-gir.git"
    },
    "bugs": {
        "url": "https://github.com/gjsify/ts-for-gir/issues"
    },
    "homepage": "https://github.com/gjsify/types/tree/main/node-gck-1#readme"
}
