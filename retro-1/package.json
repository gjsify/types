{
    "name": "@girs/retro-1",
    "version": "1.0.0-3.1.0",
    "description": "GJS TypeScript type definitions for Retro-1, generated from library version 1.0.0",
    "type": "module",
    "module": "retro-1.js",
    "main": "retro-1.js",
    "typedoc": {
        "entryPoint": "./retro-1.d.ts",
        "readmeFile": "./README.md",
        "displayName": "Retro-1",
        "tsconfig": "./tsconfig.doc.json"
    },
    "exports": {
        "./ambient": "./retro-1-ambient.d.ts",
        "./import": "./retro-1-import.d.ts",
        ".": {
            "import": {
                "types": "./retro-1.d.ts",
                "default": "./retro-1.js"
            },
            "require": {
                "types": "./retro-1.d.cts",
                "default": "./retro-1.cjs"
            }
        }
    },
    "scripts": {
        "test": "yarn test:esm && yarn test:cjs",
        "test:esm": "NODE_OPTIONS=--max_old_space_size=9216 tsc --noEmit retro-1.d.ts",
        "test:cjs": "NODE_OPTIONS=--max_old_space_size=9216 tsc --noEmit retro-1.d.cts"
    },
    "dependencies": {
        "@girs/atk-1.0": "workspace:^",
        "@girs/cairo-1.0": "workspace:^",
        "@girs/freetype2-2.0": "workspace:^",
        "@girs/gdk-3.0": "workspace:^",
        "@girs/gdkpixbuf-2.0": "workspace:^",
        "@girs/gio-2.0": "workspace:^",
        "@girs/gjs": "workspace:^",
        "@girs/glib-2.0": "workspace:^",
        "@girs/gmodule-2.0": "workspace:^",
        "@girs/gobject-2.0": "workspace:^",
        "@girs/gtk-3.0": "workspace:^",
        "@girs/harfbuzz-0.0": "workspace:^",
        "@girs/pango-1.0": "workspace:^",
        "@girs/xlib-2.0": "workspace:^"
    },
    "devDependencies": {
        "typescript": "*"
    },
    "keywords": [
        "Gir",
        "TypeScript",
        "types",
        "GObject-Introspection",
        "GJS",
        "Retro-1"
    ],
    "author": "ts-for-gir",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/gjsify/types.git"
    },
    "bugs": {
        "url": "https://github.com/gjsify/ts-for-gir/issues"
    },
    "homepage": "https://github.com/gjsify/types/tree/main/retro-1#readme"
}
