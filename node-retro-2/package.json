
{
    "name": "@girs/node-retro-2",
    "version": "2.0.0-3.0.0-beta.15",
    "description": "Node.js TypeScript type definitions for Retro-2, generated from library version 2.0.0",
    "type": "module",
    "module": "node-retro-2.js",
    "main": "node-retro-2.js",
    "typedoc": {
      "entryPoint": "./node-retro-2.d.ts",
      "readmeFile": "./README.md",
      "displayName": "Retro-2",
      "tsconfig": "./tsconfig.doc.json"
    },
    "exports": {
      "./ambient": "./node-retro-2-ambient.d.ts",
      "./import": "./node-retro-2-import.d.ts",
      ".": {
        "import": {
          "types": "./node-retro-2.d.ts",
          "default": "./node-retro-2.js"
        },
        "require": {
          "types": "./node-retro-2.d.cts",
          "default": "./node-retro-2.cjs"
        }
      }
    },
    "scripts": {
      "test": "yarn test:esm && yarn test:cjs",
      "test:esm": "tsc --noEmit node-retro-2.d.ts",
      "test:cjs": "tsc --noEmit node-retro-2.d.cts"
    },
    "dependencies": {
      
      "@girs/gtk-4.0": "workspace:^",            
      "@girs/gsk-4.0": "workspace:^",            
      "@girs/graphene-1.0": "workspace:^",            
      "@girs/gobject-2.0": "workspace:^",            
      "@girs/glib-2.0": "workspace:^",            
      "@girs/gdk-4.0": "workspace:^",            
      "@girs/cairo-1.0": "workspace:^",            
      "@girs/pangocairo-1.0": "workspace:^",            
      "@girs/pango-1.0": "workspace:^",            
      "@girs/harfbuzz-0.0": "workspace:^",            
      "@girs/freetype2-2.0": "workspace:^",            
      "@girs/gio-2.0": "workspace:^",            
      "@girs/gdkpixbuf-2.0": "workspace:^",            
      "@girs/gmodule-2.0": "workspace:^"      
    },
    "peerDependencies": {
      "node-gtk": "*"
    },
    "devDependencies": {
      "typescript": "^5.0.4"
    },
    "keywords": ["Gir", "TypeScript", "types", "GObject-Introspection", "node", "node-gtk", "Retro-2"],
    "author": "ts-for-gir",
    "license": "MIT",
    "repository": {
      "type": "git",
      "url": "git+https://github.com/gjsify/ts-for-gir.git"
