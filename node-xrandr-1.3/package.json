{
    "name": "@girs/node-xrandr-1.3",
    "version": "1.3.0-3.2.0",
    "description": "Node.js TypeScript type definitions for xrandr-1.3, generated from library version 1.3.0",
    "type": "module",
    "module": "node-xrandr-1.3.js",
    "main": "node-xrandr-1.3.js",
    "exports": {
        "./ambient": "./node-xrandr-1.3-ambient.d.ts",
        "./import": "./node-xrandr-1.3-import.d.ts",
        ".": {
            "import": {
                "types": "./node-xrandr-1.3.d.ts",
                "default": "./node-xrandr-1.3.js"
            },
            "require": {
                "types": "./node-xrandr-1.3.d.cts",
                "default": "./node-xrandr-1.3.cjs"
            }
        }
    },
    "scripts": {
        "test": "yarn test:esm && yarn test:cjs",
        "test:esm": "NODE_OPTIONS=--max_old_space_size=9216 tsc --noEmit node-xrandr-1.3.d.ts",
        "test:cjs": "NODE_OPTIONS=--max_old_space_size=9216 tsc --noEmit node-xrandr-1.3.d.cts"
    },
    "dependencies": {
        "@girs/node-gobject-2.0": "workspace:^",
        "@girs/node-gtk": "workspace:^"
    },
    "peerDependencies": {
        "node-gtk": "*"
    },
    "devDependencies": {
        "typescript": "*"
    },
    "keywords": [
        "Gir",
        "TypeScript",
        "types",
        "GObject-Introspection",
        "node",
        "node-gtk",
        "xrandr-1.3"
    ],
    "author": "ts-for-gir",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/gjsify/types.git"
    },
    "bugs": {
        "url": "https://github.com/gjsify/ts-for-gir/issues"
    },
    "homepage": "https://github.com/gjsify/types/tree/main/node-xrandr-1.3#readme"
}
