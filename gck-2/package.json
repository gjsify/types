{
    "name": "@girs/gck-2",
    "version": "4.0.0-3.0.0-beta.12",
    "description": "GJS TypeScript type definitions for Gck-2, generated from library version 4.0.0",
    "type": "module",
    "module": "gck-2.js",
    "main": "gck-2.js",
    "typedoc": {
        "entryPoint": "./gck-2.d.ts",
        "readmeFile": "./README.md",
        "displayName": "Gck-2",
        "tsconfig": "./tsconfig.doc.json"
    },
    "exports": {
        ".": {
            "import": {
                "types": "./gck-2.d.ts",
                "default": "./gck-2.js"
            },
            "require": {
                "types": "./gck-2.d.cts",
                "default": "./gck-2.cjs"
            }
        }
    },
    "scripts": {
        "test": "yarn test:esm && yarn test:cjs",
        "test:esm": "tsc --noEmit gck-2.d.ts",
        "test:cjs": "tsc --noEmit gck-2.d.cts"
    },
    "dependencies": {
        "@girs/gio-2.0": "workspace:^",
        "@girs/glib-2.0": "workspace:^",
        "@girs/gobject-2.0": "workspace:^"
    },
    "devDependencies": {
        "typescript": "^*"
    },
    "keywords": [
        "Gir",
        "TypeScript",
        "Gck-2"
    ],
    "author": "ts-for-gir",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/gjsify/ts-for-gir.git"
    },
    "bugs": {
        "url": "https://github.com/gjsify/ts-for-gir/issues"
    },
    "homepage": "https://github.com/gjsify/ts-for-gir#readme"
}
