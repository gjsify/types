{
    "name": "@girs/cogl-13",
    "version": "13.0.0-3.2.8",
    "description": "GJS TypeScript type definitions for Cogl-13, generated from library version 13.0.0",
    "type": "module",
    "module": "cogl-13.js",
    "main": "cogl-13.js",
    "exports": {
        "./ambient": {
            "types": "./cogl-13-ambient.d.ts",
            "default": "./cogl-13-ambient.js"
        },
        "./import": {
            "types": "./cogl-13-import.d.ts",
            "default": "./cogl-13-import.js"
        },
        ".": {
            "import": {
                "types": "./cogl-13.d.ts",
                "default": "./cogl-13.js"
            },
            "require": {
                "types": "./cogl-13.d.cts",
                "default": "./cogl-13.cjs"
            }
        }
    },
    "scripts": {
        "test": "yarn test:esm && yarn test:cjs",
        "test:esm": "NODE_OPTIONS=--max_old_space_size=9216 tsc --noEmit cogl-13.d.ts",
        "test:cjs": "NODE_OPTIONS=--max_old_space_size=9216 tsc --noEmit cogl-13.d.cts"
    },
    "dependencies": {
        "@girs/cairo-1.0": "workspace:^",
        "@girs/gjs": "workspace:^",
        "@girs/gl-1.0": "workspace:^",
        "@girs/glib-2.0": "workspace:^",
        "@girs/gobject-2.0": "workspace:^",
        "@girs/graphene-1.0": "workspace:^"
    },
    "devDependencies": {
        "typescript": "*"
    },
    "keywords": [
        "Gir",
        "TypeScript",
        "types",
        "GObject-Introspection",
        "GJS",
        "Cogl-13"
    ],
    "author": "ts-for-gir",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/gjsify/ts-for-gir.git"
    },
    "bugs": {
        "url": "https://github.com/gjsify/ts-for-gir/issues"
    },
    "homepage": "https://github.com/gjsify/types/tree/main/cogl-13#readme"
}
