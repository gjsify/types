{
    "name": "@girs/handy-1",
    "version": "1.0.0-4.0.0",
    "description": "GJS TypeScript type definitions for Handy-1, generated from library version 1.0.0",
    "type": "module",
    "module": "handy-1.js",
    "main": "handy-1.js",
    "exports": {
        "./ambient": {
            "types": "./handy-1-ambient.d.ts",
            "import": "./handy-1-ambient.js",
            "default": "./handy-1-ambient.js"
        },
        "./import": {
            "types": "./handy-1-import.d.ts",
            "import": "./handy-1-import.js",
            "default": "./handy-1-import.js"
        },
        ".": {
            "types": "./handy-1.d.ts",
            "import": "./handy-1.js",
            "default": "./handy-1.js"
        }
    },
    "scripts": {
        "test": "NODE_OPTIONS=--max_old_space_size=9216 tsc --noEmit handy-1.d.ts"
    },
    "dependencies": {
        "@girs/gio-2.0": "workspace:^",
        "@girs/gjs": "workspace:^",
        "@girs/gtk-3.0": "workspace:^"
    },
    "devDependencies": {
        "typescript": "*"
    },
    "keywords": [
        "Gir",
        "TypeScript",
        "types",
        "GObject-Introspection",
        "GJS",
        "Handy-1"
    ],
    "author": "ts-for-gir",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/gjsify/ts-for-gir.git"
    },
    "bugs": {
        "url": "https://github.com/gjsify/ts-for-gir/issues"
    },
    "homepage": "https://github.com/gjsify/types/tree/main/handy-1#readme"
}
