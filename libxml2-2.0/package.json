            "require": {
                "types": "./libxml2-2.0.d.cts",
                "default": "./libxml2-2.0.cjs"
            }
        }
    },
    "scripts": {
        "test": "yarn test:esm && yarn test:cjs",
        "test:esm": "tsc --noEmit libxml2-2.0.d.ts",
        "test:cjs": "tsc --noEmit libxml2-2.0.d.cts"
    },
    "dependencies": {
        "@girs/gjs": "workspace:^",
        "@girs/gobject-2.0": "workspace:^"
    },
