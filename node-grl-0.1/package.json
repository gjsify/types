{
    "name": "@girs/node-grl-0.1",
    "version": "0.1.0-3.0.3",
    "description": "Node.js TypeScript type definitions for Grl-0.1, generated from library version 0.1.0",
    "type": "module",
    "module": "node-grl-0.1.js",
    "main": "node-grl-0.1.js",
    "typedoc": {
        "entryPoint": "./node-grl-0.1.d.ts",
        "readmeFile": "./README.md",
        "displayName": "Grl-0.1",
        "tsconfig": "./tsconfig.doc.json"
    },
    "exports": {
        "./ambient": "./node-grl-0.1-ambient.d.ts",
        "./import": "./node-grl-0.1-import.d.ts",
        ".": {
            "import": {
                "types": "./node-grl-0.1.d.ts",
                "default": "./node-grl-0.1.js"
            },
            "require": {
                "types": "./node-grl-0.1.d.cts",
                "default": "./node-grl-0.1.cjs"
            }
        }
    },
    "scripts": {
        "test": "yarn test:esm && yarn test:cjs",
        "test:esm": "tsc --noEmit node-grl-0.1.d.ts",
        "test:cjs": "tsc --noEmit node-grl-0.1.d.cts"
    },
    "dependencies": {
        "@girs/node-glib-2.0": "workspace:^",
        "@girs/node-gmodule-2.0": "workspace:^",
        "@girs/node-gobject-2.0": "workspace:^",
        "@girs/node-gtk": "workspace:^"
    },
    "peerDependencies": {
        "node-gtk": "*"
    },
    "devDependencies": {
        "typescript": "*"
    },
    "keywords": [
        "Gir",
        "TypeScript",
        "types",
        "GObject-Introspection",
        "node",
        "node-gtk",
        "Grl-0.1"
    ],
    "author": "ts-for-gir",
