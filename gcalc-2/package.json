{
    "name": "@girs/gcalc-2",
    "version": "2.0.0-3.2.4",
    "description": "GJS TypeScript type definitions for GCalc-2, generated from library version 2.0.0",
    "type": "module",
    "module": "gcalc-2.js",
    "main": "gcalc-2.js",
    "exports": {
        "./ambient": "./gcalc-2-ambient.d.ts",
        "./import": "./gcalc-2-import.d.ts",
        ".": {
            "import": {
                "types": "./gcalc-2.d.ts",
                "default": "./gcalc-2.js"
            },
            "require": {
                "types": "./gcalc-2.d.cts",
                "default": "./gcalc-2.cjs"
            }
        }
    },
    "scripts": {
        "test": "yarn test:esm && yarn test:cjs",
        "test:esm": "NODE_OPTIONS=--max_old_space_size=9216 tsc --noEmit gcalc-2.d.ts",
        "test:cjs": "NODE_OPTIONS=--max_old_space_size=9216 tsc --noEmit gcalc-2.d.cts"
    },
    "dependencies": {
        "@girs/gee-0.8": "workspace:^",
        "@girs/gio-2.0": "workspace:^",
        "@girs/gjs": "workspace:^",
        "@girs/glib-2.0": "workspace:^",
        "@girs/gobject-2.0": "workspace:^"
    },
    "devDependencies": {
        "typescript": "*"
    },
    "keywords": [
        "Gir",
        "TypeScript",
        "types",
        "GObject-Introspection",
        "GJS",
        "GCalc-2"
    ],
    "author": "ts-for-gir",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/gjsify/types.git"
    },
    "bugs": {
        "url": "https://github.com/gjsify/ts-for-gir/issues"
    },
    "homepage": "https://github.com/gjsify/types/tree/main/gcalc-2#readme"
}
